                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="手机号"
                android:layout_centerInParent="true"
                android:layout_alignParentLeft="true"
                android:textColor="@color/textColorPrimary"
                android:textSize="16dp" />
